{
  "name": "akfatimeline",
  "version": "1.1.0",
  "description": "A customizable timeline component for React applications",
  "main": "src/library.js",
  "module": "src/library.js",
  "exports": {
    ".": {
      "import": "./src/library.js",
      "require": "./dist/Timeline.js",
      "default": "./src/library.js"
    },
    "./components/Timeline/Timeline.css": "./src/components/Timeline/Timeline.css"
  },
  "dependencies": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-scripts": "^5.0.1",
    "react-window": "^2.2.3",
    "uuid": "^13.0.0"
  },
  "scripts": {
    "dev": "react-scripts start",
    "start": "react-scripts start",
    "dev:clean": "react-scripts start --reset-cache",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "build:watch": "webpack --mode development --watch",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "keywords": [
    "timeline",
    "react",
    "schedule",
    "akfa",
    "Akfasoft",
    "akfatimeline",
    "Akfasoft yazılım"
  ],
  "author": "Ahmet Kürşad Aydoğan",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AKFAPLUS/akfatimeline.git"
  },
  "bugs": {
    "url": "https://github.com/AKFAPLUS/akfatimeline/issues"
  },
  "homepage": ".",
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^10.0.0",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.2",
    "postcss": "^8.4.47",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.13",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1"
  }
}
