{
  "name": "akfatimeline",
  "version": "2.2.10",
  "description": "A customizable timeline component for React applications with disableDates, custom header buttons, and past date protection features",
  "main": "./dist/library.js",
  "module": "./dist/library.js",
  "types": "./dist/library.js",
  "files": [
    "dist",
    "src",
    "README.md",
    "CHANGELOG.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/library.js",
      "import": "./dist/library.js",
      "require": "./dist/Timeline.js",
      "default": "./dist/library.js"
    },
    "./css": "./dist/components/Timeline/Timeline.css",
    "./components/Timeline/Timeline.css": "./dist/components/Timeline/Timeline.css",
    "./Timeline": {
      "types": "./dist/components/Timeline/Timeline.jsx",
      "import": "./dist/components/Timeline/Timeline.jsx",
      "require": "./dist/components/Timeline/Timeline.js",
      "default": "./dist/components/Timeline/Timeline.jsx"
    },
    "./DailyView": {
      "types": "./dist/components/Timeline/DailyView.jsx",
      "import": "./dist/components/Timeline/DailyView.jsx",
      "require": "./dist/components/Timeline/DailyView.js",
      "default": "./dist/components/Timeline/DailyView.jsx"
    },
    "./ContextMenu": {
      "types": "./dist/components/Timeline/ContextMenu.jsx",
      "import": "./dist/components/Timeline/ContextMenu.jsx",
      "require": "./dist/components/Timeline/ContextMenu.js",
      "default": "./dist/components/Timeline/ContextMenu.jsx"
    },
    "./EventDetailModal": {
      "types": "./dist/components/Timeline/EventDetailModal.jsx",
      "import": "./dist/components/Timeline/EventDetailModal.jsx",
      "require": "./dist/components/Timeline/EventDetailModal.js",
      "default": "./dist/components/Timeline/EventDetailModal.jsx"
    },
    "./EventIcon": {
      "types": "./dist/components/Timeline/EventIcon.jsx",
      "import": "./dist/components/Timeline/EventIcon.jsx",
      "require": "./dist/components/Timeline/EventIcon.js",
      "default": "./dist/components/Timeline/EventIcon.jsx"
    },
    "./EventBadge": {
      "types": "./dist/components/Timeline/EventBadge.jsx",
      "import": "./dist/components/Timeline/EventBadge.jsx",
      "require": "./dist/components/Timeline/EventBadge.js",
      "default": "./dist/components/Timeline/EventBadge.jsx"
    },
    "./LoadingSpinner": {
      "types": "./dist/components/Timeline/LoadingSpinner.jsx",
      "import": "./dist/components/Timeline/LoadingSpinner.jsx",
      "require": "./dist/components/Timeline/LoadingSpinner.js",
      "default": "./dist/components/Timeline/LoadingSpinner.jsx"
    },
    "./AutocompleteSelect": {
      "types": "./dist/components/Timeline/AutocompleteSelect.jsx",
      "import": "./dist/components/Timeline/AutocompleteSelect.jsx",
      "require": "./dist/components/Timeline/AutocompleteSelect.js",
      "default": "./dist/components/Timeline/AutocompleteSelect.jsx"
    }
  },
  "dependencies": {
    "akfatimeline": "^2.2.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-scripts": "^5.0.1",
    "react-window": "^2.2.3",
    "uuid": "^13.0.0"
  },
  "scripts": {
    "dev": "react-scripts start",
    "start": "react-scripts start",
    "dev:clean": "react-scripts start --reset-cache",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "build:watch": "webpack --mode development --watch",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "keywords": [
    "timeline",
    "react",
    "schedule",
    "akfa",
    "Akfasoft",
    "akfatimeline",
    "Akfasoft yazılım"
  ],
  "author": "Ahmet Kürşad Aydoğan",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AKFAPLUS/akfatimeline.git"
  },
  "bugs": {
    "url": "https://github.com/AKFAPLUS/akfatimeline/issues"
  },
  "homepage": ".",
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^10.0.0",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.2",
    "postcss": "^8.4.47",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.13",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1"
  }
}
